
fun main() {
  print (queMesEs(2))
}
fun queMesEs (mes: Int) : String {
    return when (mes){
        1->"enero"
        2 -> "febrero"
        3-> "marzo" 
        4-> "abril"
        5-> "mayo"
        6-> "junio"
        7-> "julio"
        8-> "agosto"
        9-> "septiembre"
        10-> "octubre"
        11-> "noviembre"
        12-> "diciembre"
        else-> "no es un mes"
    }
}
